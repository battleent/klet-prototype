{
  "name": "lck-do-dive-web-prototype",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "next": "11.1.2",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.16.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@types/react": "17.0.19",
    "@types/styled-components": "^5.1.14",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.1",
    "typescript": "4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --no-stash",
      "pre-push": "tsc"
    }
  },
  "lint-staged": {
    "*.js,jsx,ts,tsx": [
      "prettier --write",
      "eslint --quiet --fix"
    ],
    "*.{html,json}": [
      "prettier --write"
    ]
  }
}
